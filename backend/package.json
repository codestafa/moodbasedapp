{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/app.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "tsc": "tsc --noEmit --emitDeclarationOnly false --experimentalDecorators",
    "tsc:watch": "tsc --noEmit --emitDeclarationOnly false --experimentalDecorators --watch",
    "build": "rimraf dist && npx tsoa spec-and-routes && npm run tsc",
    "predev": "npm run build",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/src/app\"",
    "prestart": "npm run build",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --testTimeout=10000 --runInBand --coverage",
    "start": "cross-env NODE_ENV=production tsc && node dist/src/app",
    "tsoa:spec": "npx tsoa spec",
    "tsoa:routes": "npx tsoa routes"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/spotify-api": "^0.0.16",
    "body-parser": "^1.20.1",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "https": "^1.0.0",
    "js-cookie": "^3.0.1",
    "material-ui-audio-player": "^1.7.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node": "18.0.0",
    "passport": "^0.6.0",
    "passport-spotify": "^2.0.0",
    "querystring": "^0.2.1",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.4.0",
    "tsoa": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/consolidate": "^0.14.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^28.1.4",
    "@types/js-cookie": "^3.0.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.11",
    "@types/passport-spotify": "^2.0.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5"
  }
}
